CNC Menu
	Configure Port - TComPortOptions -cmset.pas/dfm
	Protocol Options - TProtocolOptions - ptops.pas/dfm
	Send From File - TFrmMain.MnuSendClick
	Send From Editor - TFrmMain.MnuSendfromEditorClick 
	Receive - TFrmMain.MnuReceiveClick
	Open On Receive - MnuOpenOnReceive.Checked := NOT(MnuOpenOnReceive.Checked);

Filer Options
	Filter on Open - FilterOnOpen.Checked := not FilterOnOpen.Checked;
	Filer On Send - FilterOnSend.Checked := not FilterOnSend.Checked;
	Filter On Receive - FilterOnReceive.Checked := not FilterOnReceive.Checked;
	Clear 1st Block to % - Clear1stBlock.Checked := not Clear1stBlock.Checked;
	Add % -  AddPercent.Checked := not AddPercent.Checked;
	Remove Extra Spaces - RemoveSpaces.Checked := not RemoveSpaces.Checked;
	
	
